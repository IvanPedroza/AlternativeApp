<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         id="WebApp_ID"
         version="4.0">
    <display-name>AlternativeApp</display-name>

    <welcome-file-list>
        <welcome-file>FindModelsByCity.jsp</welcome-file>
        <welcome-file>FindModelsByCounty.jsp</welcome-file>
        <welcome-file>FindModelByElectricRange.jsp</welcome-file>
        <welcome-file>CreateReview.jsp</welcome-file>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>


    <servlet>
        <servlet-name>FindModelsByCity</servlet-name>
        <servlet-class>demo.servlet.FindModelsByCity</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>FindModelsByCity</servlet-name>
        <url-pattern>/findModelsByCity/*</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>FindModelByElectricRange</servlet-name>
        <servlet-class>demo.servlet.FindModelByElectricRange</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FindModelByElectricRange</servlet-name>
        <url-pattern>/FindModelByElectricRange/*</url-pattern>
    </servlet-mapping>


</web-app>